import java.util.ArrayList;
import java.util.Collections;

public class Program {
	public static void main(String args[]){
		int n = 6;
		ArrayList<Integer> A = randomList(n);
		
		ArrayList<Integer> B = algorithm(A,n);
		//print(B);
		
		
	}
	
	public static ArrayList<Integer> algorithm(ArrayList<Integer> A,int n){
		for(int i=1;i<n;i++){
			int x = A.get(i);
			int a = 0;
			int b = i;
			while(a<b){
				int c = (a+b)/2;
				if(x<A.get(c)){
					b = c;
				}
				else{
					a = c+1;
				}
			}
			for(int j=i;j>=a;j--){
				A.set(j,A.get(j-1));
			}
			A.set(i,x);
			print(A);
		}
		return A;
	}
	
	public static void print(ArrayList<Integer> arr){
		String x = "";
		for(int i=0;i<arr.size();i++){
			x+=arr.get(i)+" ";
		}
		System.out.println(x);
	}
	
	public static void print(int x){
		System.out.println(x);
	}
	
	public static void print(String x){
		System.out.println(x);
	}
	
	public static ArrayList<Integer> randomList(int n){
	    ArrayList<Integer> myArray = ascendingList(n);
	    Collections.shuffle(myArray);
	    return myArray;
	}
	
	public static ArrayList<Integer> ascendingList(int n){
	    ArrayList<Integer> myArray = new ArrayList<>();
	    for(int i=0;i<n;i++){
	      myArray.add(i);
	    }
	    return myArray;
	}
}
